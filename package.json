{
	"name": "rust-thanks-card",
	"version": "0.0.0",
	"private": true,
	"description": "Generate a card from Rust Thanks",
	"main": "lib/main.js",
	"packageManager": "pnpm@10.16.0",
	"scripts": {
		"build": "tsc",
		"format": "biome format --write .",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"package": "ncc build --source-map --license licenses.txt",
		"test": "vitest run",
		"all": "biome ci && pnpm lint && pnpm build && pnpm test && pnpm package"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JohnTitor/rust-thanks-card.git"
	},
	"keywords": [
		"actions",
		"rust",
		"card"
	],
	"author": "Yuki Okushi",
	"license": "MIT",
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.1",
		"axios": "^1.10.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/node": "^24.0.7",
		"@vercel/ncc": "^0.38.3",
		"globals": "^16.2.0",
		"js-yaml": "^4.1.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	}
}
